{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "setup-dev": "yarn install && yarn link-all && yarn build",
    "build": "lerna run build",
    "link-all": "lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink"	
  },
  "devDependencies": {
    "lerna": "^6.4.1"
  }
}
